@article{HofriKo87,
	Abstract = {We study a data structure $\bold L$ referred to variously
		  as a padded list, a controlled density array or a sparse
		  table, containing records $\{R\sb i\}$ each uniquely
		  identified by a key $\{k(R\sb i)\}$. The data structure
		  $\bold L$ is required to support the operations
		  $\roman{Search}(\bold L, k)$, $\roman{Insert}(\bold L, k)$
		  and $\roman{Delete}(\bold L, k)$ to search, insert and
		  delete a record with key $k$. To optimize
		  $\roman{Search}(\bold L, k)$, records are stored with their
		  keys in sorted order. If the order of the keys is to be
		  maintained under insertion, records currently in $\bold L$
		  must be moved to free space. To improve the efficiency of
		  $\roman{Insert}(\bold L, k)$, records are stored in a
		  circular buffer with `gaps' so that insertion necessitates
		  moving only records up to the next gap. The array is
		  expanded and contracted during a sequence of insertions and
		  deletions depending upon the current number of gaps. In
		  this paper we assess the amount of work required to insert
		  a sequence of records.},
	Author = {Micha Hofri and Alan G. Konheim},
	Journal = {SIAM Journal on Computing},
	Pages = 1073,
	Title = {Padded lists revisited},
	Volume = 16,
	Year = 1987}

